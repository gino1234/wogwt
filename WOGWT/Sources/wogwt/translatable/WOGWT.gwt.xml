<module>

	<!-- Inherit the core Web Toolkit stuff.  -->
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.http.HTTP"/>
	<inherits name="wogwt.emulation.WOGWTEmulation"/>
	<!-- Add gwt-log support, default level `DEBUG` -->
    <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />
    <inherits name="com.googlecode.gwt.math.Math"/>
    
	<!-- This means any classes in the same package as this module file and all classes
	     in any subpackages will be translated to javascript -->
  	<source path=""/>
	<source path=""/>
	<set-property name="log_DivLogger" value="DISABLED" />
	
</module>
